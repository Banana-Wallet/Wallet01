{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "@wallet01/core#build": {
      "dependsOn": [
        "@wallet01/evm#build",
        "@wallet01/solana#build",
        "@wallet01/cosmos#build"
      ],
      "outputs": []
    },
    "@wallet01/multichain#build": {
      "dependsOn": ["@wallet01/core#build"],
      "outputs": []
    },
    "@wallet01/react#build": {
      "dependsOn": ["@wallet01/multichain#build"],
      "outputs": []
    },
    "examples": {
      "dependsOn": ["^build"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    }
  }
}
